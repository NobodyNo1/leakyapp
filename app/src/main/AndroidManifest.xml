<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.leaky.amankeldi.leakyapplication">

    <application
            android:allowBackup="true"
            android:name=".application.LeakyApplication"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activities.testleak.view.LeakyActivity"/>
        <activity
                android:name=".activities.main.StartActivity"
                android:label="@string/title_activity_start"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.testleak.view.NonLeakyActivity">
        </activity>
        <activity android:name=".activities.testleak.view.SecondNonLeakyActivity">
        </activity>
    </application>

</manifest>